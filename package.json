{
  "name": "dev-imp",
  "version": "0.0.2",
  "description": "CLI helper to get things done faster.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "npm run build; tsc --watch",
    "build": "tsc; cp package.json built/package.json; cp -r ./config ./built/; chmod +x ./built/index.js",
    "clean": "rm -rf built; rm -rf {models,system,ui}/*.js; rm -rf {models,system,ui}/*.map",
    "debug": "npm run build; cd built; echo 'chrome://inspect'; node --inspect ./index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Daviot/imp.git"
  },
  "keywords": [
    "imp",
    "cli"
  ],
  "author": "Daviot",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Daviot/imp/issues"
  },
  "homepage": "https://github.com/Daviot/imp#readme",
  "dependencies": {
    "@types/node": "^10.12.18",
    "commander": "^2.19.0",
    "enquirer": "^2.3.0",
    "fs-jetpack": "^2.2.0",
    "node-dependency-injection": "^2.2.1",
    "string-similarity": "^3.0.0",
    "terminal-kit": "^1.26.11",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "node-inspect": "^1.11.5"
  }
}
